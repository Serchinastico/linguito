{
  "name": "linguito",
  "description": "CLI tool designed to enhance the capabilities of Lingui",
  "version": "0.3.0",
  "author": "Serchinastico",
  "bin": {
    "linguito": "./bin/run.js"
  },
  "bugs": "https://github.com/madeja-studio/linguito/issues",
  "dependencies": {
    "@ai-sdk/openai": "^1",
    "@ai-sdk/openai-compatible": "^0.1.6",
    "@inkjs/ui": "^2",
    "@lingui/conf": "^5",
    "@madeja-studio/cepillo": "^0.4.0",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "ai": "^4",
    "color-json": "^3",
    "configstore": "^7",
    "espree": "^10",
    "gettext-parser": "^8",
    "ink": "^5",
    "npm-check-updates": "^17",
    "ollama-ai-provider": "^1",
    "react": "^18",
    "ts-deepmerge": "^7",
    "zod": "^3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19",
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@release-it/conventional-changelog": "^9",
    "@types/chai": "^5",
    "@types/configstore": "^6",
    "@types/espree": "^10",
    "@types/gettext-parser": "^4",
    "@types/node": "^22",
    "@types/react": "^19",
    "chai": "^5",
    "commitlint": "^19",
    "eslint": "^9",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "eslint-plugin-chai-friendly": "^1",
    "eslint-plugin-perfectionist": "^4",
    "eslint-plugin-prettier": "^5",
    "lefthook": "^1",
    "oclif": "^4",
    "prettier": "^3",
    "release-it": "^17",
    "shx": "^0.3.4",
    "ts-morph": "^25",
    "tsc-alias": "^1",
    "tsx": "^4",
    "typescript": "^5",
    "typescript-eslint": "^8",
    "vitest": "^3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/madeja-studio/linguito",
  "keywords": [
    "lingui",
    "i18n",
    "internacionalization",
    "l10n",
    "localization",
    "translation",
    "cli",
    "utility",
    "assistant",
    "llm",
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "linguito",
    "dirname": "linguito",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "repository": "madeja-studio/linguito",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && tsc-alias",
    "deps": "npm-check-updates",
    "deps:update": "npm-check-updates -u",
    "docs:gif": "tsx scripts/backup-catalog-demo-files backup && vhs docs/demo.tape && mv demo.gif docs && tsx scripts/backup-catalog-demo-files restore",
    "lint": "eslint \"**/*.{js,ts}\"",
    "lint:commit": "commitlint --edit",
    "lint:fix": "eslint \"**/*.{js,ts}\" --fix",
    "prepare": "lefthook install",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "oclif manifest",
    "release": "release-it",
    "release:major": "release-it --increment=major",
    "release:minor": "release-it --increment=minor",
    "release:patch": "release-it --increment=patch",
    "start": "npm run build && ./bin/dev.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
